er-radius: 50px;\r\n  padding: 0 10px;\r\n  outline: none;\r\n  /* box-shadow: 0px 0px 5px black; */\r\n}\r\n.globalHeader .searchForm > button {\r\n  white-space: nowrap;\r\n  padding: 0 15px;\r\n  margin-left: 8px;\r\n  border-radius: 10px;\r\n  border: 1px solid #ddd;\r\n  background: #fff;\r\n}\r\n.globalMain > .Mainwrapper {\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  max-width: 400px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n.Mainwrapper li {\r\n  width: 83px;\r\n  /* margin-right: 28px; */\r\n  margin: 10px 20px 0 25px;\r\n  /* border: 1px solid red; */\r\n}\r\n/* .Mainwrapper li{\r\n   margin: 0px 10px;\r\n} */\r\n.globalMain li > .wrapper {\r\n  width: 80px;\r\n  height: 80px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 50%;\r\n  background-color: white;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: relative;\r\n}\r\n.globalMain .wrapper > .logo {\r\n  font-size: 44px;\r\n  font-weight: 500;\r\n}\r\n.Mainwrapper .text {\r\n  font-size: 14px;\r\n  text-align: center;\r\n  margin-top: 4px;\r\n  margin-bottom: 10px;\r\n  color: #fff;\r\n}\r\n/* .Mainwrapper .addWrapper{\r\n    border: 1px solid black;\r\n} */\r\n.Mainwrapper .addWrapper .iconWrapper {\r\n  font-size: 64px;\r\n  width: 80px;\r\n  height: 80px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 50%;\r\n  background-color: white;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.wrapper:hover .close{\r\n display: block;\r\n cursor: default;\r\n}\r\n.wrapper .close {\r\n  position: absolute;\r\n  top: 4px;\r\n  right: 14px;\r\n  display: none;\r\n}\r\n@media (min-width: 480px) {\r\n  body {\r\n    height:100vh;\r\n}\r\n  .globalHeader {\r\n    margin-top: 100px;\r\n    margin-bottom: 60px;\r\n  }\r\n  .searchForm {\r\n    max-width: 400px;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n  }\r\n  .globalMain > .Mainwrapper{\r\n    max-width: 720px;\r\n\r\n  }\r\n  .Mainwrapper li {\r\n    width: 83px;\r\n    margin: 10px 10px 16px 44px;\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"fd3c3100fbe9ed326c79701f8d8cbe41","cacheData":{"env":{}}}